#!/bin/sh
#SBATCH -N 100
#SBATCH --time=04:00:00

mpiexec \
  --allow-run-as-root \
  --mca plm_rsh_args "-p 2222" \
  --oversubscribe \
  -np 5 \
  --output-filename out/p \
  -mca btl_tcp_if_include eth1 \
  -mca oob_tcp_if_include eth1 \
  -mca btl self,tcp \
  --hostfile /tmp/mpihosts \
  /root/parsplice
